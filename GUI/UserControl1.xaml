                                <DataGridTextColumn Width="auto" Header="Host Count" Binding ="{Binding HostCount}" IsReadOnly="True">
                                <DataGridTextColumn.CellStyle>
                                        <Style TargetType="DataGridCell">
                                            <Setter Property="ToolTip" Value="Indicates the number of hosts configured through WWPN through the same portset." />
                                        </Style>
                                    </DataGridTextColumn.CellStyle>
                                </DataGridTextColumn>
                                <DataGridTextColumn Width="auto" Header="Active Login Count" Binding ="{Binding ActiveLoginCount}" IsReadOnly="True">
                                <DataGridTextColumn.CellStyle>
                                        <Style TargetType="DataGridCell">
                                            <Setter Property="ToolTip" Value="Indicates the number of active login counts through WWPN. For a configured host, blocked login will not be included." />
                                        </Style>
                                    </DataGridTextColumn.CellStyle>
                                </DataGridTextColumn>
                                <DataGridTextColumn Width="auto" Header="Attachment" Binding ="{Binding Attachment}" IsReadOnly="True">
                                <DataGridTextColumn.CellStyle>
                                        <Style TargetType="DataGridCell">
                                            <Setter Property="ToolTip" Value="Indicates if the port is attached to an FC switch or directly to an FC host." />
                                        </Style>
                                    </DataGridTextColumn.CellStyle>
                                </DataGridTextColumn>
                            </DataGrid.Columns>
                        </DataGrid>
                        </StackPanel>  
                    </Grid>
                </StackPanel> 
                </Grid> 
            </ScrollViewer>
        </GroupBox>
    </Grid>
</UserControl>