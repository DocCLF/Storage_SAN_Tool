                            <Grid.RowDefinitions>
                                <RowDefinition Height="100"/>
                                <RowDefinition Height="auto"/>
                                <RowDefinition Height="25"/>
                            </Grid.RowDefinitions>
                            <Grid >
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="70" />
                                    <ColumnDefinition />
                                </Grid.ColumnDefinitions>
                                <Ellipse Width="65" Height="65" Fill="WhiteSmoke" Stretch="Uniform" >
                                    <Ellipse.Effect>
                                        <DropShadowEffect Direction="320" ShadowDepth="2" Color="LightBlue" BlurRadius="5" />
                                    </Ellipse.Effect>
                                </Ellipse>
                                <Ellipse Width="60" Height="60" >
                                   <Ellipse.Fill>
                                       <ImageBrush ImageSource = "{Binding IBMSTOIcon , Mode=TwoWay}" Stretch="Uniform" />
                                   </Ellipse.Fill>
                                </Ellipse>
                                <TextBlock
                                    x:Name="TB_STO_DevOne"
                                    Grid.Column="1"
                                    Style="{DynamicResource DashBoardTB1}"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    Text=" " 
                                    Margin="8,0,5,0"
                                    Visibility = "Collapsed"/>
                            </Grid>
                            <Grid Grid.Row="1">
                                <StackPanel Margin="5,0">
                                    <Grid Margin="0,0,0,2">
                                        <TextBlock Text="45" />
                                        <TextBlock Text="100" HorizontalAlignment="Right"/>
                                    </Grid>
                                    <ProgressBar Height="5" BorderBrush="WhiteSmoke" Foreground="Teal" Value="45">
                                        <ProgressBar.Resources>
                                            <Style TargetType="{x:Type Border}">
                                                <Setter Property="CornerRadius" Value="2" />
                                            </Style>
                                        </ProgressBar.Resources>
                                    </ProgressBar>
                                </StackPanel>
                            </Grid>
                            <Grid Grid.Row="2">
                                <StackPanel Orientation="Horizontal" Margin="5,3,5,0" ToolTip="Last Changes">
                                    <Image Source = "{Binding ClockIcon96 , Mode=TwoWay}" Stretch="Uniform" VerticalAlignment="Bottom" HorizontalAlignment="Left" MaxHeight="30" />
                                    <TextBlock x:Name="TB_Clock_STO_DevOne" Text="" HorizontalAlignment="Right" Margin="5,3" />
                                </StackPanel>
                            </Grid>
                        </Grid>
